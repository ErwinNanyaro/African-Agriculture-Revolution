def find_common_features(df1, df2):
    """Find common features between two dataframes"""
    common_cols = list(set(df1.columns) & set(df2.columns))
    
    print(f"\nüîç COMMON FEATURES FOUND: {len(common_cols)}")
    
    # Categorize common features
    demographic = [col for col in common_cols if any(term in str(col).lower() 
                   for term in ['age', 'education', 'gender'])]
    
    social = [col for col in common_cols if any(term in str(col).lower() 
              for term in ['group', 'trust', 'share', 'network'])]
    
    resource = [col for col in common_cols if any(term in str(col).lower() 
                for term in ['access', 'resource', 'training'])]
    
    print(f"   üë• Demographic: {len(demographic)} features")
    print(f"   ü§ù Social: {len(social)} features")
    print(f"   üõ†Ô∏è Resource: {len(resource)} features")
    
    return common_cols

