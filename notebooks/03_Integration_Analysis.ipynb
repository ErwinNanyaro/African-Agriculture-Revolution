def standardize_strawberry_data(df):
    """Standardize Morogoro strawberry data"""
    df_std = df.copy()
    
    # Add region identifier
    df_std['region'] = 'Morogoro'
    df_std['crop_type'] = 'Strawberry'
    df_std['data_source'] = 'Strawberry Survey'
    
    # Standardize key columns
    if '_1_What_is_your_age' in df_std.columns:
        df_std['age_group'] = df_std['_1_What_is_your_age']
    
    if '_2_What_is_your_level_of_education' in df_std.columns:
        df_std['education'] = df_std['_2_What_is_your_level_of_education']
    
    # Create simplified yield metric
    yield_cols = [col for col in df_std.columns if 'yield' in str(col).lower()]
    if yield_cols:
        yield_col = yield_cols[0]
        # Map yield to numeric
        yield_mapping = {
            'very_low': 1,
            'low': 2,
            'moderate': 3,
            'high': 4,
            'very_high': 5
        }
        df_std['yield_score'] = df_std[yield_col].map(yield_mapping)
    
    return df_std

def standardize_arusha_data(df):
    """Standardize Arusha multi-crop data"""
    df_std = df.copy()
    
    # Add region identifier
    df_std['region'] = 'Arusha'
    df_std['crop_type'] = 'Multiple Crops'
    df_std['data_source'] = 'Climate Adaptation Survey'
    
    # Standardize key columns
    # Find age column
    age_cols = [col for col in df_std.columns if 'age' in str(col).lower()]
    if age_cols:
        df_std['age_group'] = df_std[age_cols[0]].astype(str)
    
    # Find education column
    edu_cols = [col for col in df_std.columns if 'education' in str(col).lower()]
    if edu_cols:
        df_std['education'] = df_std[edu_cols[0]]
    
    # Create adaptation score
    adaptation_indicators = []
    for col in df_std.columns:
        if any(term in str(col).lower() for term in ['extension', 'training', 'technology', 'climate', 'adapt']):
            if df_std[col].dtype == 'object':
                # Convert yes/no to numeric
                df_std[f'{col}_score'] = df_std[col].apply(lambda x: 1 if str(x).lower() == 'yes' else 0)
                adaptation_indicators.append(f'{col}_score')
    
    if adaptation_indicators:
        df_std['adaptation_score'] = df_std[adaptation_indicators].sum(axis=1)
    
    return df_std

# Standardize both datasets
