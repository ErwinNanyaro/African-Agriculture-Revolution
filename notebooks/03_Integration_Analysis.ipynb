    # Simplify education categories
    def simplify_education(edu):
        if pd.isna(edu):
            return 'Unknown'
        edu_str = str(edu).lower()
        if 'primary' in edu_str:
            return 'Primary'
        elif 'secondary' in edu_str:
            return 'Secondary'
        elif 'tertiary' in edu_str or 'higher' in edu_str:
            return 'Tertiary'
        elif 'no' in edu_str:
            return 'No Formal'
        else:
            return 'Other'
    
    integrated_df['education_simple'] = integrated_df['education'].apply(simplify_education)
    
    edu_region = pd.crosstab(integrated_df['education_simple'], integrated_df['region'])
    
