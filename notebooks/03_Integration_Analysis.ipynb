print("\nüåç REGIONAL COMPARISON ANALYSIS")

def compare_regions(df):
    """Compare Morogoro and Arusha farmers"""
    
    comparisons = []
    
    # 1. Age distribution
    if 'age_group' in df.columns:
        age_comparison = df.groupby('region')['age_group'].apply(
            lambda x: x.value_counts().index[0] if len(x.value_counts()) > 0 else 'Unknown'
        )
        comparisons.append(('Most Common Age Group', age_comparison))
    
    # 2. Education level
    if 'education' in df.columns:
        edu_comparison = df.groupby('region')['education'].apply(
            lambda x: x.value_counts().index[0] if len(x.value_counts()) > 0 else 'Unknown'
        )
        comparisons.append(('Most Common Education', edu_comparison))
    
    # 3. Group membership
    group_cols = [col for col in df.columns if any(term in str(col).lower() 
                   for term in ['group', 'member', 'association'])]
    
    if group_cols:
        group_col = group_cols[0]
        if df[group_col].dtype == 'object':
            group_pct = df.groupby('region')[group_col].apply(
                lambda x: (x == 'yes').mean() * 100 if 'yes' in x.values else 0
            )
            comparisons.append(('Group Membership (%)', group_pct))
    
    # Create comparison table
    if comparisons:
        comparison_df = pd.DataFrame(dict(comparisons))
        print("\nüìä REGIONAL COMPARISON TABLE:")
        print(comparison_df)
    
    return comparisons

regional_comparisons = compare_regions(integrated_df)

