# 5. EXTRACT KEY INSIGHTS FROM ARUSHA DATA
# ============================================

def extract_arusha_insights(df):
    """Extract revolutionary insights from Arusha data"""
    print("\nüí° EXTRACTING ARUSHA-SPECIFIC INSIGHTS")
    
    insights = {}
    
    # 1. Extension Service Access
    if 'Do you have access to extension services from government agencies or NGOs?' in df.columns:
        ext_access = df['Do you have access to extension services from government agencies or NGOs?'].value_counts()
        insights['extension_access'] = {
            'yes': ext_access.get('Yes', 0),
            'no': ext_access.get('No', 0),
            'pct_yes': (ext_access.get('Yes', 0) / len(df)) * 100
        }
        print(f"   üìä Extension Access: {insights['extension_access']['pct_yes']:.1f}% have access")
    
    # 2. Training Frequency
    training_cols = [col for col in df.columns if 'training' in str(col).lower() or 'workshop' in str(col).lower()]
    if training_cols:
        training_col = training_cols[0]
        training_counts = df[training_col].value_counts()
        print(f"   üéì Training Frequency: Most common = {training_counts.index[0] if len(training_counts) > 0 else 'N/A'}")
    
    # 3. Climate Adaptation Strategies
    strategy_cols = [col for col in df.columns if 'strategy' in str(col).lower() or 'adapt' in str(col).lower()]
    if strategy_cols:
        strategy_col = strategy_cols[0]
        # Count strategies (they're comma-separated)
        all_strategies = []
        for strategies in df[strategy_col].dropna():
            if isinstance(strategies, str):
                strategies_list = strategies.split(',')
                all_strategies.extend([s.strip() for s in strategies_list])
        
        from collections import Counter
        strategy_counts = Counter(all_strategies)
        insights['top_strategies'] = strategy_counts.most_common(5)
        
        print(f"   üå± Top Climate Adaptation Strategies:")
        for strategy, count in insights['top_strategies']:
            pct = (count / len(df)) * 100
            print(f"     ‚Ä¢ {strategy}: {pct:.1f}% of farmers")
    
    # 4. Social Capital Indicators
    sc_cols = [col for col in df.columns if any(term in str(col).lower() 
               for term in ['member', 'trust', 'share', 'cooperat', 'network'])]
    if sc_cols:
        insights['social_capital_indicators'] = len(sc_cols)
        print(f"   ü§ù Social Capital Indicators: {len(sc_cols)} found")
    
    return insights

# Extract insights
insights = extract_arusha_insights(cleaned_data)

