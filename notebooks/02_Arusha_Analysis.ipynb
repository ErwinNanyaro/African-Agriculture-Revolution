# Create adaptation score
def calculate_adaptation_score(df):
    """Calculate climate adaptation score"""
    df_score = df.copy()
    
    # Initialize score
    df_score['adaptation_score'] = 0
    
    # 1. Extension services (1 point if yes)
    ext_cols = [col for col in df.columns if 'extension' in str(col).lower()]
    if ext_cols:
        ext_col = ext_cols[0]
        df_score['adaptation_score'] += df_score[ext_col].apply(lambda x: 1 if str(x).lower() == 'yes' else 0)
    
    # 2. Training attendance
    training_cols = [col for col in df.columns if 'training' in str(col).lower()]
    if training_cols:
        training_col = training_cols[0]
        # Map frequency to points
        training_points = {
            'Never attend': 0,
            'Once a year': 1,
            '2-3 times a year': 2,
            'More than 3 times a year': 3
        }
        df_score['adaptation_score'] += df_score[training_col].map(training_points).fillna(0)
    
    # 3. Financial access
    finance_cols = [col for col in df.columns if 'financial' in str(col).lower() or 'loan' in str(col).lower()]
    if finance_cols:
        finance_col = finance_cols[0]
        df_score['adaptation_score'] += df_score[finance_col].apply(lambda x: 1 if str(x).lower() == 'yes' else 0)
    
    # 4. Technology access
    tech_cols = [col for col in df.columns if 'technology' in str(col).lower()]
    if tech_cols:
        tech_col = tech_cols[0]
        df_score['adaptation_score'] += df_score[tech_col].apply(lambda x: 1 if str(x).lower() == 'yes' else 0)
    
    # Normalize score to 0-10 scale
    if df_score['adaptation_score'].max() > 0:
        df_score['adaptation_score_normalized'] = (df_score['adaptation_score'] / df_score['adaptation_score'].max()) * 10
    
    return df_score

# Calculate scores
arusha_with_scores = calculate_adaptation_score(cleaned_data)

if 'adaptation_score_normalized' in arusha_with_scores.columns:
    print(f"   üìà Adaptation Score Range: {arusha_with_scores['adaptation_score_normalized'].min():.1f} - {arusha_with_scores['adaptation_score_normalized'].max():.1f}")
    print(f"   üìä Average Adaptation Score: {arusha_with_scores['adaptation_score_normalized'].mean():.1f}/10")
    
    # Create adaptation categories
    conditions = [
        arusha_with_scores['adaptation_score_normalized'] <= 3,
        arusha_with_scores['adaptation_score_normalized'] <= 6,
        arusha_with_scores['adaptation_score_normalized'] > 6
    ]
    choices = ['Low Adaptation', 'Medium Adaptation', 'High Adaptation']
    
    arusha_with_scores['adaptation_category'] = np.select(conditions, choices, default='Unknown')
    
    print(f"\n   üéØ Adaptation Categories:")
    for category in ['Low Adaptation', 'Medium Adaptation', 'High Adaptation']:
        count = (arusha_with_scores['adaptation_category'] == category).sum()
        pct = (count / len(arusha_with_scores)) * 100
        print(f"     {category}: {count} farmers ({pct:.1f}%)")

